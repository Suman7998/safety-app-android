<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Include user settings for cloud backup -->
        <include domain="sharedpref" path="WomenSafetySettings.xml"/>
    </cloud-backup>
    
    <device-transfer>
        <!-- Include user settings for device transfer -->
        <include domain="sharedpref" path="WomenSafetySettings.xml"/>
        
        <!-- Exclude sensitive emergency data -->
        <exclude domain="database" path="emergency_contacts.db"/>
    </device-transfer>
</data-extraction-rules>
